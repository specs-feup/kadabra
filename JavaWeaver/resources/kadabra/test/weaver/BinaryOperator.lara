import weaver.Query;

aspectdef BinaryOperator

	for(var $op of Query.search('binaryExpression').get()) {
		println("operator: " + $op.operator);
		println("operands: " + $op.operands);
		println("lhs: " + $op.lhs);
		println("rhs: " + $op.rhs);
	
	}

	// Ast of the operator copy
	println(Query.search('binaryExpression').getFirst().copy().ast);
